<!DOCTYPE html>
<html lang="en">
  <head>
    <link href="/static/mermaid.min.css" rel="stylesheet"/>
  </head>
  <body>
    <div id="wrapper"></div>
    <script src="/static/grid.umd.js"></script>
    <script>
     new gridjs.Grid({
         search: true,
         columns: [
             {
                 name: "id",
                 width: "100px",
                 formatter: (cell) => {
                     return gridjs.html(
                         `<a href='/${cell}'>M${cell}</a>`
                     );
                 }
             },
             {
                 name: "visible",
                 width: "100px",
                 filter: true
             },
             {
                 name: "hour angle",
                 width: "100px",
             },
             {
                 name: "name"
             },
         ],
         sort: true,
         data: {{.Objects}}
     }).render(document.getElementById("wrapper"));
    </script>
  </body>
</html>

